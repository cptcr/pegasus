{
  "name": "pegasus-discord-bot",
  "version": "1.0.0",
  "description": "A powerful Discord bot with advanced features",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "pegasus"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.6.5",
    "canvas": "^3.1.2",
    "chalk": "^4.1.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.3",
    "i18next": "^23.7.16",
    "i18next-fs-backend": "^2.3.1",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.26.0",
    "ms": "^2.1.3",
    "nanoid": "^5.0.4",
    "node-cron": "^3.0.3",
    "postgres": "^3.4.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "drizzle-kit": "^0.20.10",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
